	<!-- Start with the Parking Brake on -->
	<controls>
		<engines>
			<engine n="0">
				<throttle type="double">0</throttle>
				<reheat type="double">0</reheat>
			</engine>
			<engine n="1">
				<throttle type="double">0</throttle>
        			<reheat type="double">0</reheat>
