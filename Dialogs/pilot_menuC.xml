<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<default>
		<menu n="3">
			<item>
				<name>autopilot</name>
				<label>Autopilot Settings</label>
				<binding>
					<command>nasal</command>
					<script>
						var autopilot = gui.Dialog.new("sim/gui/dialogs/autopilot/dialog", "Aircraft/JPack/Voodoomaster/Dialogs/autopilot9.xml");
						autopilot.open();
					</script>
				</binding>
			</item>
		</menu>
		<menu n="10">
			<label>Loreley-C</label>
			<enabled type="bool">true</enabled>

			<item>
				<label>Autostart</label>
				<binding>
					<command>nasal</command>
					<script>
						Loreley.autostart();
					</script>
				</binding>
			</item>

			<item>
				<label>Select Livery</label>
				<binding>
					<command>nasal</command>
					<script>
						aircraft.livery.dialog.toggle()
					</script>
				</binding>
			</item>

			<item>
				<label>Test dialog</label>
				<binding>
					<command>nasal</command>
					<script>
						voodoomaster.apdialog.show();
					</script>
				</binding>
			</item>
		</menu>
	</default>

</PropertyList>

